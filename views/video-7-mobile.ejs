<script>
        var palette = new Rickshaw.Color.Palette();

var graph = new Rickshaw.Graph( {
        element: document.querySelector("#chart"),
        width: 400,
        height: 250,
        renderer: 'line',
        series: [
                {
                        name: "Views",
                        data: [  { x: 1394409600, y: 242 }, { x: 1396137600, y: 3627 }, { x: 1397520000, y: 6830 }, { x: 1398816000, y: 10357 } 
                        ],
                        color: palette.color()
                },
                {
                        name: "Shares",
                        data: [  { x: 1394409600, y: 553 }, { x: 1396137600, y: 4058 }, { x: 1397520000, y: 9412 }, { x: 1398816000, y: 12722 } 
                        ],
                        color: palette.color()
                },
                {
                        name: "Likes",
                        data: [  { x: 1394409600, y: 382 }, { x: 1396137600, y: 4024 }, { x: 1397520000, y: 9815 }, { x: 1398816000, y: 14381 } 
                        ],
                        color: palette.color()
                }
               
        ]
} );

var x_axis = new Rickshaw.Graph.Axis.Time( { graph: graph } );

var y_axis = new Rickshaw.Graph.Axis.Y( {
        graph: graph,
        orientation: 'left',
        tickFormat: Rickshaw.Fixtures.Number.formatKMBT,
        element: document.getElementById('y_axis'),
} );

graph.render();

</script>


